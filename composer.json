{
    "name": "itkg/phporchestra-media-bundle",
    "description": "A media bundle for the PhpOrchestra Project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Thal",
            "email": "nico.th4l@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/itkg/phporchestra-base-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/itkg/phporchestra-model-interface"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-imagick": "*",
        "itkg/phporchestra-model-interface": "*",
        "doctrine/data-fixtures":"1.0.0",
        "doctrine/mongodb-odm": "1.0.0-BETA11",
        "doctrine/mongodb-odm-bundle" : "3.0.0-BETA6",
        "stof/doctrine-extensions-bundle": "1.1.0",
        "knplabs/knp-gaufrette-bundle": "v0.1.7"
    },
    "require-dev": {
        "itkg/phporchestra-base-bundle": "dev-master",
        "scrutinizer/ocular": "1.1.1",
        "symfony/validator": "2.5.5",
        "jmikola/geojson": "1.0.1",
        "phpunit/phpunit" : "4.0.14",
        "phake/phake": "2.0.0-beta2"
    },
    "config" : {
        "bin-dir" : "bin"
    },
    "autoload": {
        "psr-0": {
            "PHPOrchestra\\MediaBundle": "",
            "PHPOrchestra\\Media": ""
        }
    },
    "target-dir": "PHPOrchestra"
}
